.Bar {
  composes: clear from "../../style/helpers.mcss";
  background: rgb(39, 61, 64);
  overflow: visible;
  display: flex;
}

.left,
.center,
.right {
  float: left;
  min-height: 1px;
  flex: none;
}

.center {
  text-align: center;
  flex: auto;
}

.right {
  text-align: right;
}

.item {
  display: inline-block;
  padding: 15px;
}
