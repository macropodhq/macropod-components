@import "../icon-button/style.pcss";

$color-button-delete-hover: white;
$color-button-delete-background-hover: #d46060;


.DeleteButton {
  cursor: pointer;
  -webkit-touch-callout: none;
  user-select: none;
  path {
    fill: $color-icon-button-background;
  }

  &:focus, &:hover {
    path {
      fill: $color-button-delete-background-hover;
    }
  }
}
